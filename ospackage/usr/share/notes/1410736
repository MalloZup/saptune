# 1410736 - TCP/IP: setting keepalive interval
# Description:    Set keepalive interval
# Version 3 from 25.11.2009 in English

[version]
# SAP-NOTE=1410736 VERSION=3 DATE=25.11.2009 NAME="TCP/IP: setting keepalive interval"

[sysctl]
net.ipv4.tcp_keepalive_time <= 300
net.ipv4.tcp_keepalive_intvl <= 300
